{
    "version": 2,
    "builds": [
        {
        "src": "KopiBudget.Ui/package.json",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "dist/kopiBudget.ui",
            "buildCommand": "cd KopiBudget.Ui && npm install && npm run build"
        }
        }
    ],
    "routes": [
        {
        "src": "/(.*)",
        "dest": "/index.html"
        }
    ]
}
